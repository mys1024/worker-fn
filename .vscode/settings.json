{
  "deno.enable": true,
  "deno.lint": true,
  "deno.unstable": false,
  "deno.cacheOnSave": true,
  "deno.disablePaths": ["npm"],
  "[javascript]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "cSpell.words": [
    "changelogithub",
    "codecov",
    "deno",
    "denoland",
    "lcov",
    "minzip",
    "mrpc",
    "okikio",
    "pnpx",
    "runtimes",
    "Transferables",
    "tsup",
    "typeof"
  ]
}
