{
  "name": "worker-fn",
  "version": "",
  "description": "worker-fn hides the complexity of communication between the main thread and Worker threads, making it easy to call functions defined in the Worker.",
  "type": "module",
  "author": "mys1024",
  "packageManager": "pnpm@8.6.1",
  "license": "MIT",
  "homepage": "https://github.com/mys1024/worker-fn#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mys1024/worker-fn.git"
  },
  "keywords": [
    "web",
    "worker",
    "Web Workers",
    "function",
    "mirror",
    "proxy",
    "browser",
    "Deno"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx tsup src/main.ts --clean --dts --format esm"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "devDependencies": {
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}
